function findMinLengthOfThreeWords(word1, word2, word3) {
  //your code here
  if (word1.length < word2.length) {
   if (word1.length < word3.length) {
      return word1.length;
   }}
  else {
      return word2.length;
 
 
}}

var output = findMinLengthOfThreeWords('a', 'be', 'see');
console.log(output);
